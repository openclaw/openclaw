# OpenClaw Agent System - Environment template
# Copy to .env and fill in values.

# Database (use SQLite for local dev without Docker)
DATABASE_URL=sqlite:///./openclaw_agents.db
# DATABASE_URL=postgresql://user:pass@localhost:5432/openclaw_agents

# Server
DEBUG=false
SECRET_KEY=change-me-use-openssl-rand-hex-32

# AI Providers â€” set keys for any providers you want agents to use
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# GOOGLE_API_KEY=
# GROQ_API_KEY=
# DEEPSEEK_API_KEY=
# MISTRAL_API_KEY=
# XAI_API_KEY=
# OLLAMA_BASE_URL=http://localhost:11434

# OpenClaw Gateway (routes AI requests through the gateway)
# OPENCLAW_GATEWAY_URL=http://localhost:18789

# Provider preference order (comma-separated, first available wins)
# AI_PROVIDER_PREFERENCE=openai,anthropic,groq,deepseek,ollama

# Service Integrations
# GITHUB_TOKEN=
# STRIPE_API_KEY=
# NOTION_TOKEN=
# TELEGRAM_BOT_TOKEN=

# Redis
# REDIS_PASSWORD=openclaw_redis_dev

# Celery (when using worker/beat)
# CELERY_BROKER_URL=redis://:openclaw_redis_dev@localhost:6379/0
# CELERY_RESULT_BACKEND=redis://:openclaw_redis_dev@localhost:6379/0
