# Copy to phala-deploy/.env.rollout-targets and edit for your environment.
# Load before running:
#   set -a; source phala-deploy/.env.rollout-targets; set +a
#   ./phala-deploy/cvm-rollout-targets.sh all --wait

# OpenClaw CVM(s)
PHALA_OPENCLAW_CVM_IDS="replace-with-openclaw-cvm-uuid"
PHALA_OPENCLAW_COMPOSE_FILE=phala-deploy/docker-compose.yml
PHALA_OPENCLAW_DEPLOY_ENV_FILE=/tmp/openclaw-phala-deploy.env
PHALA_OPENCLAW_DEPLOY_SECRETS="MASTER_KEY REDPILL_API_KEY S3_BUCKET S3_ENDPOINT S3_PROVIDER S3_REGION AWS_ACCESS_KEY_ID AWS_SECRET_ACCESS_KEY"

# mux-server CVM(s)
PHALA_MUX_CVM_IDS="replace-with-mux-cvm-uuid"
PHALA_MUX_COMPOSE_FILE=phala-deploy/mux-server-compose.yml
PHALA_MUX_DEPLOY_ENV_FILE=/tmp/mux-phala-deploy.env
# MUX_REGISTER_KEY must match OpenClaw gateway.http.endpoints.mux.registerKey.
# MUX_ADMIN_TOKEN is required for control-plane pairing token issuance.
# Add optional keys (MUX_JWT_PRIVATE_KEY) if needed.
PHALA_MUX_DEPLOY_SECRETS="MUX_REGISTER_KEY MUX_ADMIN_TOKEN TELEGRAM_BOT_TOKEN DISCORD_BOT_TOKEN"
