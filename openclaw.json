{
  "gateway": {
    "port": 8080,
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "controlUi": {
      "dangerouslyDisableDeviceAuth": true
    }
  },
  "agents": {
    "defaults": {
      "skipBootstrap": true,
      "model": { "primary": "anthropic/claude-sonnet-4-5" },
      "workspace": "/home/node/.openclaw/workspace",
      "heartbeat": {}
    },
    "list": [{
      "id": "bea",
      "default": true,
      "identity": { "name": "Bea" }
    }]
  },
  "channels": {
    "slack": {
      "enabled": true,
      "mode": "http",
      "signingSecret": "${SLACK_SIGNING_SECRET}",
      "botToken": "${SLACK_BOT_TOKEN}",
      "webhookPath": "/slack/events",
      "groupPolicy": "open"
    }
  },
  "cron": {
    "enabled": true,
    "maxConcurrentRuns": 2
  },
  "skills": {
    "entries": {
      "e-conomic": {
        "enabled": true,
        "env": {
          "ECONOMIC_APP_SECRET_TOKEN": "${ECONOMIC_APP_SECRET_TOKEN}",
          "ECONOMIC_AGREEMENT_GRANT_TOKEN": "${ECONOMIC_AGREEMENT_GRANT_TOKEN}"
        }
      }
    }
  }
}
