{
  "id": "canvas-lms",
  "name": "Canvas LMS",
  "description": "Canvas LMS integration plugin for OpenClaw.",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "baseUrl": {
        "type": "string"
      },
      "token": {
        "type": "string"
      },
      "defaultPerPage": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "uiHints": {
    "token": {
      "sensitive": true,
      "label": "Canvas API Token"
    },
    "baseUrl": {
      "label": "Canvas Base URL",
      "placeholder": "https://canvas.example.edu"
    },
    "defaultPerPage": {
      "label": "Default Page Size"
    }
  }
}
